    },
    "android": {
      "package": "se.lindskogen.hakkerneues"
    },
    "extra": {
      "bugsnag": {
        "apiKey": "842ce72724a50b3e83e895b167bbb622"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "@bugsnag/expo/hooks/post-publish.js",
          "config": {}
        }
      ]
    }
  }
}
